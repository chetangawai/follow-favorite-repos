{
    "root": true,
    "extends": [
      "react-app",
      "plugin:react/recommended",
    ],
    "plugins": [ "react-hooks", "react", "prettier"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "jsx-quotes": 2,
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react/display-name": 0,
      "react/jsx-no-undef": 2,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/jsx-wrap-multilines": 2,
      "react/no-did-mount-set-state": 0,
      "react/no-did-update-set-state": 1,
      "react/no-is-mounted": 2,
      "react/prefer-es6-class": 1,
      "react/react-in-jsx-scope": 2,
      "react/self-closing-comp": 2,
      "react/sort-comp": 2,
      "block-scoped-var": 2,
      "camelcase": 1,
      "comma-dangle": [
          "error",
          {
            "arrays": "always-multiline",
            "exports": "always-multiline",
            "functions": "never",
            "imports": "always-multiline",
            "objects": "always-multiline"
          }
      ],
      "react/prop-types": [
        2,
        {
          "ignore": ["children", "className", "id", "style"]
        }
      ],
      "complexity": ["error", { "max": 160 }],
      "consistent-return": 2,
      "curly": 2,
      "default-case": 2,
      "dot-notation": 2,
      "eol-last": 2,
      "eqeqeq": 2,
      "guard-for-in": 2,
      "indent": ["error", 2, { "SwitchCase": 1, "flatTernaryExpressions": true, "MemberExpression": 1 }],
      "no-alert": 1,
      "no-caller": 2,
      "no-console": 2,
      "no-debugger": 2,
      "no-div-regex": 2,
      "no-dupe-keys": "error",
      "no-else-return": 2,
      "no-eq-null": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 2,
      "no-fallthrough": 2,
      "no-floating-decimal": 2,
      "no-implied-eval": 2,
      "no-iterator": 2,
      "no-labels": 2,
      "no-lone-blocks": 2,
      "no-loop-func": 2,
      "no-multi-spaces": 2,
      "no-multi-str": 2,
      "no-native-reassign": 2,
      "no-new-func": 2,
      "no-new-wrappers": 2,
      "no-new": 2,
      "no-octal-escape": 2,
      "no-octal": 2,
      "no-path-concat": 2,
      "no-process-env": 0,
      "no-proto": 2,
      "no-redeclare": 2,
      "no-restricted-imports": ["error", { "paths": [{ "name": "lodash", "importNames": ["default"], "message": "Please don't use default lodash import. Use lodash: `import { method } from 'lodash'`" }] }],
      "no-return-assign": 2,
      "no-script-url": 2,
      "no-self-compare": 2,
      "no-sequences": 1,
      "no-shadow": 1,
      "no-undef": 2,
      "no-underscore-dangle": 0,
      "no-unneeded-ternary": "error",
      "no-unused-expressions": 2,
      "no-unused-vars": 2,
      "no-var": 2,
      "no-void": 2,
      "no-warning-comments": 1,
      "no-with": 2,
      "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "es5" }],
      "quotes": [2, "single", { "avoidEscape": true }],
      "object-shorthand": 2,
      "radix": 2,
      "semi": 2,
      "strict": 0,
      "vars-on-top": 0,
      "wrap-iife": [2, "inside"],
    }
  }
  